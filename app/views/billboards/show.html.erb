<nav>
  <%= link_to 'Home', root_path %>
  <%= link_to 'Billboards', billboards_path %>
  <%= button_to 'Log Out', destroy_user_session_path, method: :delete%>
</nav>

<h1><%= @billboard.name %></h1>
<h3><%= link_to "Back to the Billboard charts", billboards_path %></h3>
<hr />

<%= link_to 'View Artist',  billboard_artists_path(@billboard) %>
<%= link_to "Edit Billboard", edit_billboard_path %>
<%= link_to 'Delete Billboard', @billboard, method: :delete %>

<ul>
  <% @billboard.artists.each do |artist| %>
    <li>
      <%= link_to artist.name, [@billboard, artist] %>
    </li>
  <% end %>
</ul>
  
